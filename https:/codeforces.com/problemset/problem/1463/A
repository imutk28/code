#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int z, arr[3];
 
int main() {
    cin.tie(0);
    cin >> z;
    while (z--) {
        cin >> arr[0] >> arr[1] >> arr[2];
        sort(arr, arr + 3);
        int sum = arr[0] + arr[1] + arr[2];
        if (sum % 9 == 0) 
        {
            if (sum / 9 > arr[0]) 
            {
                cout << "NO" <<endl;
            }
            else cout << "YES" <<endl;
        }
        else cout << "NO\n";
    }
}
 
 
 
